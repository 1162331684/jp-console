<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jeeplus.qc.invoice.mapper.InvoiceSetupMapper">

    <select id="findById" resultType="com.jeeplus.qc.invoice.service.dto.InvoiceDTO">
        SELECT
        *
        FROM invoice_sequence_setup a WHERE a.id = #{id} and a.del_flag = 0
    </select>
</mapper>